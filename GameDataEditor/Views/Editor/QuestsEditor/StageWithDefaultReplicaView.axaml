<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="700" d:DesignHeight="600"
             x:Class="GameDataEditor.Views.Editor.QuestsEditor.StageWithDefaultReplicaView">

  <Border Background="WhiteSmoke" Padding="20,10" Width="500">
    <StackPanel Spacing="10">
      
      <TextBlock Classes="h3" Text="Default Replica"/>
      <TextBlock Text="replicaType"/>
      <ComboBox Items="{Binding replicaTypes}"
                SelectedItem="{Binding selectedReplicaType}"/>
      <TextBlock Text="nextStage"/>
      <TextBox Text="{Binding stage.nextStage}"/>

    </StackPanel>
  </Border>
  
</UserControl>
